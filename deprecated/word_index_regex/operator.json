{
    "description": "Run regex on word_index",
    "component": "com.sap.system.python3Operator",
    "inports": [
        {
            "name": "words",
            "type": "message.table",
            "description": "Message table."
        }
    ],
    "outports": [
        {
            "name": "log",
            "type": "string",
            "description": "Logging data"
        },
        {
            "name": "data",
            "type": "message.table",
            "description": "Table after regex"
        }
    ],
    "config": {
        "$type": "http://sap.com/vflow/textanalysis.word_index_regex.configSchema.json",
        "script": "file://word_index_regex.py",
        "debug_mode": true,
        "word_type": "P",
        "language_filter": "None",
        "patterns": "\"\\d{2}:\\d{2}:\\d{2}\",\"\\d{3}-\", \"^[-'\\./+]\", \"[-\\./+]$\""
    },
    "tags": {
        "sdi_utils": ""
    },
    "iconsrc": "regex_cleansing.svg"
}