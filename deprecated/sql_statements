-- SELECT WORD FROM (SELECT WORD, COUNT("COUNT") AS WORD_COUNT FROM WORD_INDEX WHERE LANGUAGE = 'DE' GROUP BY "WORD") WHERE WORD_COUNT < 5;
-- DELETE FROM WORD_INDEX WHERE WORD = '00:00:00';

SELECT COUNT(*) FROM (SELECT AM."DATE", WI."LANGUAGE", "WORD", "TYPE", sum("COUNT") AS "COUNT" FROM "WORD_INDEX"  AS WI
INNER JOIN "ARTICLES_METADATA" AS AM ON WI."HASH_TEXT" = AM."HASH_TEXT" WHERE WI."LANGUAGE" = 'DE'
GROUP BY "DATE", "WORD","TYPE",WI."LANGUAGE");
